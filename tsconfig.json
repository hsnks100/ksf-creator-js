{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "moduleResolution": "node",
        "rootDir": "./",
        "outDir": "js",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true 
    },
    "include": [
        "*"
    ],
    "exclude": [
        "node_modules"
    ]
}
